{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "cathedral:cathedral/x_rail_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "cathedral:cathedral/x_rail_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "cathedral:cathedral/x_rail_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "cathedral:cathedral/x_rail_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "cathedral:cathedral/x_rail_side", "y": 270, "uvlock": true }
        },
		{   "when": { "postcap": "true" },
            "apply": { "model": "cathedral:cathedral/x_rail_cap" }
        }
    ]
}
